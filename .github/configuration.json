{
  "template": "#{{CHANGELOG}}\n\n<details>\n<summary>ğŸ“¦ Uncategorized & Raw Changes</summary>\n\n#{{UNCATEGORIZED}}\n</details>\n\n## ğŸ¤ Contributors\n- #{{CONTRIBUTORS}}",
  "categories": [
    {
      "title": "## ğŸš€ Features",
      "labels": ["feature", "feat"]
    },
    {
      "title": "## ğŸ› Bug Fixes",
      "labels": ["fix", "bug"]
    },
    {
      "title": "## âš¡ Performance",
      "labels": ["perf"]
    },
    {
      "title": "## ğŸ§ª Tests",
      "labels": ["test", "tests"]
    },
    {
      "title": "## ğŸ“¦ Dependencies",
      "labels": ["dependencies", "deps"]
    },
    {
      "title": "## ğŸ“š Documentation",
      "labels": ["docs", "doc"]
    },
    {
      "title": "## ğŸ”¨ Refactoring",
      "labels": ["refactor", "style"]
    },
    {
      "title": "## âš™ï¸ CI/CD",
      "labels": ["ci", "build", "workflow"]
    },
    {
      "title": "## ğŸ§¹ Maintenance",
      "labels": ["chore"],
      "exclude_labels": ["deps", "dependencies"]
    },
    {
      "title": "## ğŸ’¬ Other",
      "labels": ["other", "misc", "revert"]
    }
  ],
  "label_extractor": [
    {
      "pattern": "^chore\\((deps)\\): .*",
      "target": "$1"
    },
    {
      "pattern": "^(build|chore|ci|docs|feat|fix|perf|refactor|revert|style|test|deps|other|misc){1}(\\([\\w\\-\\.]+\\))?(!)?: ([\\w ])+([\\s\\S]*)",
      "target": "$1"
    }
  ],
  "sort": {
    "order": "ASC",
    "on_property": "mergedAt"
  },
  "pr_template": "- #{{TITLE}}",
  "empty_template": "- no changes",
  "max_pull_requests": 1000,
  "max_back_track_time_days": 1000
}
